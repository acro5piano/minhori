{
  "name": "minhori",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:acro5piano/minhori",
  "author": "gosho-kazuya <ketsume0211@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel start:react start:server",
    "start:react": "cross-env NODE_ENV=development webpack-dev-server --watch --progress",
    "start:server": "nodemon api/* --watch api --exec 'ts-node -r tsconfig-paths/register api/index.ts'",
    "start:production": "ts-node -r tsconfig-paths/register api/index.ts",
    "build": "cross-env NODE_ENV=production webpack",
    "make:seed": "yarn knex seed:make",
    "make:migration": "yarn knex migrate:make",
    "db:init": "node tasks/init-db.js",
    "db:cli": "pgcli postgres://postgres:postgres@127.0.0.1:29576/larkin_local",
    "db:seed": "yarn knex seed:run",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:rollback": "yarn knex migrate:rollback",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/react-jss": "^8.6.1",
    "@types/styled-components": "^4.1.8",
    "@types/uuid": "^3.4.4",
    "@types/webpack-env": "^1.13.7",
    "@types/winston": "^2.4.4",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express-winston": "^3.1.0",
    "knex": "^0.16.3",
    "knex-tiny-logger": "^1.1.0",
    "objection": "^1.5.3",
    "pg": "^7.8.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3",
    "tsconfig-paths": "^3.8.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^5.0.8",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "cross-env": "^5.2.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "resolutions": {
    "ajv": "6.8.1"
  },
  "jest": {
    "testRegex": ".*\\.test\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "^apotte-assets/(.*)$": "<rootDir>/assets/$1",
      "^apotte/(.*)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/"
  }
}
